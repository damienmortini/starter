{
  "name": "@damienmortini/starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "concurrently --kill-others \"npm run build:watch\" \"npm run serve\"",
    "serve": "wds --node-resolve --watch",
    "test": "eslint packages",
    "build": "npx nx run-many --target=build",
    "build:watch": "npx nx run-many --target=build --watch",
    "update": "npm i",
    "upgrade": "ncu --deep -u"
  },
  "author": "Damien Mortini",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@damienmortini/eslint-config": "^0.0.15",
    "@damienmortini/prettier-config": "^0.0.1",
    "@damienmortini/typescript-config": "^0.0.5",
    "@web/dev-server": "^0.3.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "npm-check-updates": "^16.13.2",
    "nx": "16.7.4",
    "nx-cloud": "latest"
  }
}
