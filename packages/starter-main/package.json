{
  "name": "@damienmortini/starter-main",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc && ncp src dist --filter=\"^[^.]+.((?!ts$).)*$\"",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*",
        "tsconfig.json"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "dependencies": {
    "lit-html": "^3.3.0"
  },
  "devDependencies": {
    "@damienmortini/typescript-config": "^0.0.12",
    "ncp": "^2.0.0",
    "typescript": "^5.8.3",
    "wireit": "^0.14.12"
  }
}
